{
  "name": "react-elmish",
  "version": "1.0.0",
  "description": "Elmish for React using Typescript",
  "author": "atheck",
  "license": "MIT",
  "dependencies": {},
  "peerDependencies": {
    "react": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.13.10",
    "@babel/core": "^7.13.13",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/preset-env": "^7.13.12",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.3",
    "jest": "^26.6.3",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  },
  "scripts": {
    "build": "npm run build:types && npm run build:js",
